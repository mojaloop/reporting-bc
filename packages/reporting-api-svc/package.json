{
    "name": "@mojaloop/reporting-bc-reporting-api-svc",
    "version": "0.3.33",
    "description": "mojaloop reporting api service",
    "license": "Apache-2.0",
    "homepage": "https://github.com/mojaloop/reporting-bc/",
    "private": false,
    "mojaloop": {
        "publish_to_dockerhub": true
    },
    "bugs": {
        "url": "https://github.com/mojaloop/reporting-bc/issues#readme"
    },
    "contributors": [
        "Zwe Htet Myat <zwehtet.myat@thitsaworks.com>"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "start": "tsc && npm run service",
        "start:dev": "tsc && EXAMPLE_ENV_VAR=value npm run service",
        "service": "node dist/application/index.js",
        "build": "tsc",
        "watch": "tsc -w",
        "clean": "npm run clean:npm; npm run clean:dist",
        "clean:dist": "rm -rf dist",
        "clean:npm": "rm -rf node_modules",
        "pretest": "npm run build",
        "standard": "npm run lint",
        "lint": "npm run build && npx eslint ./src",
        "lint:fix": "npm run build && npx eslint ./src --fix",
        "test:unit": "jest --passWithNoTests",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -u"
    },
    "dependencies": {
        "@mojaloop/auditing-bc-client-lib": "~0.0.15",
        "@mojaloop/auditing-bc-public-types-lib": "~0.0.17",
        "@mojaloop/logging-bc-client-lib": "^0.1.30",
        "@mojaloop/logging-bc-public-types-lib": "~0.1.23",
        "@mojaloop/platform-configuration-bc-client-lib": "~0.5.7",
        "@mojaloop/platform-configuration-bc-public-types-lib": "~0.5.2",
        "@mojaloop/platform-shared-lib-nodejs-kafka-client-lib": "^0.3.71",
        "@mojaloop/platform-shared-lib-observability-types-lib": "~0.1.9",
        "@mojaloop/security-bc-client-lib": "~0.5.12",
        "@mojaloop/security-bc-public-types-lib": "~0.3.6",
        "@mojaloop/participant-bc-public-types-lib": "~0.5.5",
        "exceljs": "^4.4.0",
        "express": "^4.18.3",
        "express-validator": "^7.0.1",
        "mongodb": "~5.5.0"
    },
    "devDependencies": {
        "@mojaloop/logging-bc-public-types-lib": "^0.1.22",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.4.4",
        "@types/node": "^18.11.19",
        "@types/node-cache": "^4.2.5",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "eslint": "^8.33.0",
        "jest": "^29.4.3",
        "npm-check-updates": "^16.6.5",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    }
}
